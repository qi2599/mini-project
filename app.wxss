page{
  color: #54637d;
  background-color: rgb(242, 242, 242);
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.clearfix:after, .clearfix:before{
  content: "";
  display: table;
  clear: both;
}

