  url('//at.alicdn.com/t/font_1124787_s9pxwzn7sq.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1124787_s9pxwzn7sq.woff') format('woff'),
  url('//at.alicdn.com/t/font_1124787_s9pxwzn7sq.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1124787_s9pxwzn7sq.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
